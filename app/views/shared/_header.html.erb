<section id="welcome">
<nav class="navbar navbar-default">
  <div class="container-fluid">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">YourNewGear.com</a>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

      <ul class="nav navbar-nav navbar-right">
        <% unless brand_signed_in? %>
          <li><%= link_to 'Sign In', brand_session_path %></li>
          <li><%= link_to 'Sign Up', new_brand_registration_path %></li>
        <% else %>
           <li><%= link_to 'Manage Products', brand_path(current_brand.id) %></li>
           <li><%= link_to 'Company Settings', edit_brand_registration_path %></li>
           <li><%= link_to 'Sign Out',  destroy_brand_session_path, method: :delete %></li>
        <% end %>
        <% if admin_signed_in? %>
          <li><%= link_to 'Manage Brands', brands_path %></li>
          <li><%= link_to 'Admin Sign Out', destroy_admin_session_path, method: :delete %></li>
        <%# else %>
          <!-- <li><%#= link_to 'Log into Admin', new_admin_session_path %></li> -->
        <% end %>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->
</nav>
</section>